{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "prod": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "cross-env NODE_ENV=development node ./bin/www",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-messages": "^1.0.1",
    "express-promise-router": "^3.0.1",
    "express-session": "^1.15.6",
    "hbs": "^4.0.0",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "morgan": "^1.7.0",
    "node-localstorage": "^1.3.0",
    "nodemon": "^1.15.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "tedious": "^2.4.0",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "supertest": "^2.0.0"
  }
}
